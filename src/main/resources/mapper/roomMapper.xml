<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.zzti.soft.scores.dao.RoomDao">

	<delete id="delTeaRoom" parameterType="java.util.List">
		DELETE FROM tea_room WHERE
		<foreach collection="list" item="item" index="index" separator="or">
			id = #{item }
		</foreach>
	</delete>
	
	<insert id="addTeaRoom" parameterType="java.util.List">
		INSERT tea_room(room_id, identity_id, identity_name, tea_room.`start`, tea_room.`end`) 
		VALUES
		<foreach collection="list" item="item" index="index" separator="," >  
			(#{item.room_id}, #{item.identity_id}, #{item.identity_name}, #{item.start}, #{item.end})  
		</foreach> 
	</insert>
	
</mapper>